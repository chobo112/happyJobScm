	@Override
	public int itemUpdateJson(Map<String, Object> paramMap, MultipartFile[] files) throws Exception {
		
		ManagementModel orginFile = managementDao.itemDetail(paramMap);//itemDetail 확인
        if (orginFile.getMajor_class() != null) {//파일이름 컬럼명 확인 
            File oldFile = new File(orginFile.getItem_note());//파일의 phsycal_path 확인
            oldFile.delete();
        }
        
        String itemFilePath = prducimage + File.separator;
		FileUtilMultipartFile fileUpload = new FileUtilMultipartFile(files, rootPath, virtualRootPath, itemFilePath); 
		Map<String, Object> fileInfo = fileUpload.uploadFiles();
		if(!fileInfo.isEmpty()){
			paramMap.put("fileYn", "Y");
			paramMap.put("fileInfo", fileInfo);			
		}else{
			paramMap.put("fileYn", "N");
			paramMap.put("fileInfo", null);						
		}
	
		return managementDao.itemUpdateJson(paramMap);
	}

